{
  "manifest_version": 2,
  "name": "Ashraf Medium Opener",
  "description": "This extension opens medium links.",
  "version": "1.0",
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}